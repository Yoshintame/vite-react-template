{
	"name": "vite-react-template",
	"author": "Mikhail Ivanov <m.ivanov0427@gmail.com>",
	"description": "A production ready, batteries included starter template for Vite + React projects",
	"keywords": [
		"vite",
		"react",
		"boilerplate",
		"starter",
		"template"
	],
	"repository": {
		"type": "git",
		"url": ""
	},
	"license": "MIT",
	"version": "1.0.0",
	"scripts": {
		"dev": "vite --host --port 4000",
		"build": "vite build",
		"preview": "vite preview --host",
		"format": "prettier src/**/*.{ts,tsx} --write",
		"bundle-visualizer": "vite-bundle-visualizer",
		"lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 --fix",
		"typecheck": "tsc --noEmit",
		"storybook": "storybook dev -p 6006",
		"storybook:setup": "node node_modules/@storybook/addon-styling/bin/postinstall.js",
		"storybook:build": "storybook build",
		"test": "vitest run src/ && playwright test",
		"test:setup": "npx playwright install",
		"test:unit": "vitest src/",
		"test:unit:coverage": "vitest --coverage src/",
		"test:e2e": "playwright test",
		"test:e2e:report": "playwright show-report",
		"commitlint": "commitlint --edit",
		"commitizen": "exec < /dev/tty && git cz --hook || true",
		"prepare": "husky install",
		"preinstall": "npx only-allow pnpm",
		"lint-staged": "pnpm exec lint-staged"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.3.4",
		"@tanstack/react-query": "^4.36.1",
		"@tanstack/react-table": "^8.11.3",
		"@tanstack/router": "0.0.1-beta.119",
		"axios": "^1.6.2",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"path-to-regexp": "^6.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.49.3",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.7",
		"zod": "^3.22.4",
		"zustand": "^4.4.7"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.8.1",
		"@commitlint/config-conventional": "^17.8.1",
		"@hookform/devtools": "^4.3.1",
		"@playwright/test": "^1.40.1",
		"@storybook/addon-essentials": "^7.6.7",
		"@storybook/addon-interactions": "^7.6.7",
		"@storybook/addon-links": "^7.6.7",
		"@storybook/addon-styling": "^1.3.7",
		"@storybook/blocks": "^7.6.7",
		"@storybook/react": "^7.6.7",
		"@storybook/react-vite": "^7.6.7",
		"@storybook/testing-library": "0.0.14-next.2",
		"@tanstack/react-query-devtools": "^4.36.1",
		"@tanstack/react-table-devtools": "^8.11.3",
		"@tanstack/router-devtools": "0.0.1-beta.119",
		"@testing-library/jest-dom": "^5.17.0",
		"@testing-library/react": "^14.1.2",
		"@testing-library/user-event": "^14.5.2",
		"@total-typescript/ts-reset": "^0.4.2",
		"@types/node": "^20.10.8",
		"@types/react": "^18.2.47",
		"@types/react-dom": "^18.2.18",
		"@types/testing-library__jest-dom": "^5.14.9",
		"@typescript-eslint/eslint-plugin": "^6.18.1",
		"@typescript-eslint/parser": "^6.18.1",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"@vitest/coverage-v8": "^0.33.0",
		"autoprefixer": "^10.4.16",
		"commitizen": "^4.3.0",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-unicorn": "^48.0.1",
		"husky": "^8.0.3",
		"jsdom": "^22.1.0",
		"lint-staged": "^14.0.1",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-tailwindcss": "^0.5.11",
		"storybook": "^7.6.7",
		"tailwindcss": "^3.4.1",
		"typescript": "^5.3.3",
		"vite": "^4.5.1",
		"vite-bundle-visualizer": "^1.0.0",
		"vitest": "^0.33.0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}